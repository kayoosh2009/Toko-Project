{
  "manifest_version": 3,
  "name": "Toko New Tab",
  "version": "1.0",
  "description": "Расширение: Кастомная страница новой вкладки для проекта Toko.",
  "chrome_url_overrides": {
    "newtab": "browser.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [],
  "host_permissions": [],
  "web_accessible_resources": [
    {
      "resources": ["icon.png"],
      "matches": ["<all_urls>"]
    }
  ]
}
